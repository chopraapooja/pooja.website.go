               data-25p-top="opacity: 1;"
               data-0p-top="opacity: 1;"
               data--10p-top="opacity: 0;" />
          <img src="/assets_site/images/index/section2/1_business_continuity.svg" class="card_image"
               data-anchor-target="#card2"
               data-80p-top="opacity: 0;"
               data-55p-top="opacity: 1;"
               style="opacity: 1;" />
        </div>
      </div>
    </div>
  </div>
</section>


